{
  "version": 2,
  "functions": {
    "api/x-integration/tweets.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30,
      "memory": 1024
    },
    "api/x-integration/stats.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/x-integration/(.*)",
      "dest": "/api/x-integration/"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=180, stale-while-revalidate=60"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/x-integration/tweets.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/x-integration/stats.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/x-integration/tweets",
      "destination": "/api/x-integration/tweets.js"
    },
    {
      "source": "/api/x-integration/stats",
      "destination": "/api/x-integration/stats.js"
    }
  ]
}